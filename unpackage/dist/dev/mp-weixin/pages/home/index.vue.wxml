<template name="58c25168">
  <view class="_view data-v-f26f1696">
    <view class="_view data-v-f26f1696 container {{[isIphoneX ? 'containerX' : '']}}">
      <view class="_view data-v-f26f1696 shouye_top">
        <form bindsubmit="handleProxy" reportSubmit="true" data-eventid="{{'ORj-0'}}" data-comkey="{{$k}}" class="_form data-v-f26f1696">
          <button class="_button data-v-f26f1696 shouye_top_left" formType="submit">
            <image class="_image data-v-f26f1696 shouye_coinImg" src="../../assets/images/luck/icon_coin2.png"></image>
            <text class="_text data-v-f26f1696">{{luckySum}}</text>
            <image class="_image data-v-f26f1696 shouye_rightImg" src="../../assets/images/luck/icon_right1.png"></image>
          </button>
        </form>
        <form bindsubmit="handleProxy" reportSubmit="true" wx:if="{{isSign}}" data-eventid="{{'I7Q-2'}}" data-comkey="{{$k}}" class="_form data-v-f26f1696">
          <button class="_button data-v-f26f1696 shouye_top_right shouye_top_right1" formType="submit">已签到
            <image class="_image data-v-f26f1696 sign_rightImg1" src="../../assets/images/icon_right.png"></image>
          </button>
        </form>
        <form bindsubmit="handleProxy" reportSubmit="true" wx:else data-eventid="{{'ozf-1'}}" data-comkey="{{$k}}" class="_form data-v-f26f1696">
          <button class="_button data-v-f26f1696 shouye_top_right" formType="submit">
            <image class="_image data-v-f26f1696 sign_rightImg" src="../../assets/images/icon_sign.png"></image> 签到
            <image animation="{{bubbleAc}}" class="_image data-v-f26f1696 sign_bubbleImg" src="../../assets/images/icon_signbubble.png"></image>
          </button>
        </form>
      </view>
      <view class="_view data-v-f26f1696 listContainer">
        <form bindsubmit="handleProxy" reportSubmit="true" wx:key="index" key="{{index}}" data-eventid="{{'IES-3-'+index}}" data-comkey="{{$k}}" wx:for="{{lists}}" wx:for-index="index" wx:for-item="item" class="_form data-v-f26f1696">
          <button class="_button data-v-f26f1696 indexBtn" data-data="{{item}}" data-lotteryid="{{item.id}}" formType="submit">
            <view class="_view data-v-f26f1696 index_list">
              <view class="_view data-v-f26f1696 list_img" style=" {{('height:' + (imgHeight + 'px') + ';')}}">
                <image class="_image data-v-f26f1696 lotteryImg" src="{{item.thumb}}" style=" {{('height:' + (imgHeight + 'px') + ';')}}"></image>
                <image class="_image data-v-f26f1696 partStatusImg" src="../../assets/images/icon_part.png" wx:if="{{item.join_status == 1}}"></image>
                <view class="_view data-v-f26f1696 list_tag" wx:if="{{item.sponsor_name}}">{{item.sponsor_name}} 赞助</view>
              </view>
              <view class="_view data-v-f26f1696 list_content">
                <block wx:if="{{item.is_more == 1}}">
                  <view class="_view data-v-f26f1696 list_lottery" wx:key="key" key="{{key}}" wx:for="{{item.prizes}}" wx:for-index="key" wx:for-item="value">{{value}}</view>
                </block>
                <view class="_view data-v-f26f1696 list_lottery" wx:else>奖品：{{item.title}}×{{item.count}}</view>
                <view class="_view data-v-f26f1696 list_rule" wx:if="{{item.open_type == 1}}">{{item.endtime_text}} 自动开奖</view>
                <view class="_view data-v-f26f1696 list_rule" wx:if="{{item.open_type == 2}}">{{item.join_total}}人数开奖</view>
                <view class="_view data-v-f26f1696 list_rule" wx:if="{{item.open_type == 3}}">手动开奖</view>
              </view>
            </view>
          </button>
        </form>
        <view class="_view data-v-f26f1696 welfare" wx:if="{{recommendations.length > 0}}">
          <view class="_view data-v-f26f1696 welfare_title">推荐抽奖</view>
          <view class="_view data-v-f26f1696 welfare_container">
            <form bindsubmit="handleProxy" reportSubmit="true" wx:key="index" key="{{index}}" data-eventid="{{'Hpw-4-'+index}}" data-comkey="{{$k}}" wx:for="{{recommendations}}" wx:for-index="index" wx:for-item="item" class="_form data-v-f26f1696">
              <button class="_button data-v-f26f1696 indexBtn1 {{[index == 0 ? 'welfare_container2' : '']}}" data-data="{{item}}" data-lotteryid="{{item.id}}" formType="submit">
                <view class="_view data-v-f26f1696 welfare_container1">
                  <view class="_view data-v-f26f1696 welfare_container_left">
                    <image class="_image data-v-f26f1696 welfareImg" src="{{item.thumb}}"></image>
                    <image class="_image data-v-f26f1696 joinStatusImg" src="../../assets/images/icon_part.png" wx:if="{{item.join_status == 1}}"></image>
                  </view>
                  <view class="_view data-v-f26f1696 welfare_container_right">
                    <view class="_view data-v-f26f1696 wc_right_title">奖品：{{item.title}}×{{item.count}}</view>
                    <view class="_view data-v-f26f1696 wc_right_sponsor" wx:if="{{item.sponsor_name}}">{{item.sponsor_name}} 赞助</view>
                    <view class="_view data-v-f26f1696 wc_right_sponsor1" wx:else></view>
                    <view class="_view data-v-f26f1696 wc_right_time" wx:if="{{item.open_type == 1}}">{{item.endtime_text}} 自动开奖</view>
                    <view class="_view data-v-f26f1696 wc_right_time" wx:if="{{item.open_type == 2}}">{{item.join_total}}人数开奖</view>
                    <view class="_view data-v-f26f1696 wc_right_time" wx:if="{{item.open_type == 3}}">手动开奖</view>
                  </view>
                </view>
              </button>
            </form>
          </view>
        </view>
      </view>
      <form bindsubmit="handleProxy" reportSubmit="true" data-eventid="{{'bHG-5'}}" data-comkey="{{$k}}" class="_form data-v-f26f1696">
        <button class="_button data-v-f26f1696 homePage indexshouye" formType="submit">我要上首页></button>
      </form>
      <view class="_view data-v-f26f1696 isPhone" style="height:80px" wx:if="{{isIos}}"></view>
    </view>
  </view>
</template>